{
  "version": 2,
  "builds": [
      {
          "src": "*.py",
          "use": "@vercel/python"
      }
  ],
  "routes": [
      {
          "src": "(.*)",
          "dest": "app.py"
      }
  ]
  "functions": {
    "app/api/**/*": {
      "maxDuration": 500 // All functions can run for a maximum of 5 seconds
    }
}
